Select tutor_name, round(avg(grade)) as average_grade
from student, tutor
where student.tutor_id = tutor.tutor_id
group by tutor_name
order by average_grade;

select course_name, (max(age) - min(age)) as range_age, max(age) as max_age
from course, student
where course.course_code = student.course_code
group by course_name;

select module_name, round(avg(grade)) as average_grade
from
(select student_name, module_name, grade
from student, module, student_module
where student_module.module_code = module.module_code AND
student_module.student_no = student.student_no
order by student.grade)
group by module_name
order by average_grade

select course_name, module_name
from course, module, course_module
where course_module.course_code = course.course_code And
course_module.module_code = module.module_code
AND (Module_Name = 'Web Development' or Module_Name = 'Database Systems')
