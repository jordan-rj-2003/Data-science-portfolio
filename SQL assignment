CREATE TABLE Course (Course_Code VARCHAR(10) NOT NULL, Course_Name VARCHAR (20)
NOT NULL, Dept_No INT NOT NULL, CONSTRAINT Course_Course_Code_pk PRIMARY
KEY(Course_Code), CONSTRAINT Course_Dept_No_fk FOREIGN KEY(Dept_No) REFERENCES
Department(Dept_No)); CREATE TABLE Tutor (Tutor_Id INT NOT NULL, Tutor_Name VARCHAR
(20) NOT NULL, Dept_No INT NOT NULL, CONSTRAINT Tutor_Tutor_Id_pk PRIMARY
KEY(Tutor_Id), CONSTRAINT Tutor_Dept_No_fk FOREIGN KEY(Dept_No) REFERENCES
Department(Dept_No)); CREATE TABLE Department (Dept_No INT NOT NULL, Dept_Name
VARCHAR (20) NOT NULL, CONSTRAINT Department_Dept_No_pk PRIMARY KEY(Dept_No));
CREATE TABLE Student (Student_No VARCHAR(10) NOT NULL, Student_Name VARCHAR (20)
NOT NULL, CONSTRAINT Student_Student_No_pk PRIMARY KEY(Student_No)); CREATE TABLE
Module (Module_Code VARCHAR(10) NOT NULL, Module_Name VARCHAR (20) NOT NULL,
CONSTRAINT Module_Module_Code_pk PRIMARY KEY(Module_Code)); CREATE TABLE
Student_Module (Module_Code VARCHAR(10) NOT NULL, Student_No VARCHAR(10) NOT
NULL, CONSTRAINT Student_Module_Module_Code_fk FOREIGN KEY(Module_Code)
REFERENCES Module(Module_Code), CONSTRAINT Student_Module_Student_No_fk FOREIGN
KEY(Student_No) REFERENCES Student(Student_No)); CREATE TABLE Course_Module
(Module_Code VARCHAR(10) NOT NULL, Course_Code VARCHAR(10) NOT NULL, CONSTRAINT
Course_Module_Module_Code_fk FOREIGN KEY(Module_Code) REFERENCES
Module(Module_Code), CONSTRAINT Course_Module_Course_Code_fk FOREIGN
KEY(Course_Code) REFERENCES Course(Course_Code)); ALTER TABLE Student ADD
Course_Code VARCHAR(10) NOT NULL; ALTER TABLE Student ADD CONSTRAINT
Student_Course_Code_fk FOREIGN KEY(Course_Code) REFERENCES Course(Course_Code);
ALTER TABLE Student ADD GRADE INT NULL; Alter table Tutor add Tutor_Age INT NULL ALTER
